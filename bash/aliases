# editor
# alias subl="/Applications/Sublime\ Text\ 2.app/Contents/SharedSupport/bin/subl"

# git
alias gitree="git log --graph --pretty=format:'%an: %s - %Cred%h%Creset %C(yellow)%d%Creset %Cgreen(%cr)%Creset' --abbrev-commit --date=relative"

# ls
alias ls="ls -F"
alias l="ls -lAh"
alias ll="ls -l"
alias la='ls -A'

# misc
alias reload='. ~/.zshrc'
alias ports="sudo lsof -iTCP -sTCP:LISTEN -P"
alias pubkey="more ~/.ssh/id_rsa.public | pbcopy | echo '=> Public key copied to clipboard.'"

alias tunneldev="ssh -v -N devtunnel"
alias tunneldatamining="ssh -v -N dataminingtunnel"
alias tunneloxa="ssh -v -N oxatunnel"
alias tunnelikoula="ssh -v -N ikoulatunnel"
